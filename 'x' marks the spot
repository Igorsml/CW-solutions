const xMarksTheSpot = (input) => {  
  if (!input.length) {
    return [];
  } 
  const result = [];
  
  for(let i = 0; i < input.length; i += 1) {
    for(let j = 0; j < input[i].length; j += 1) {
      if (input[i][j] === 'x') {
        result.push(i,j)
      }
    }
  }
  if (result.length > 2) {
    return [];
  }
  return result;
}
